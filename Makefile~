#######################################################Compilação##################################################################

all: msg test

msg:
	@echo Gremlin_Alfa-estamos compilando vosso codigo mestre
test:
	g++ -Wall -std=c++11 -I	include/ source/main.cpp source/memory.cpp -o bin/test -lsfml-graphics -lsfml-window -lsfml-system

########################################################Execução###################################################################
roll:
	./bin/test

##########################################################FIM######################################################################

